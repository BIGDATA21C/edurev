{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
      "insight-ui": {
        "root": "",
        "sourceRoot": "src",
        "projectType": "application",
        "prefix": "app",
        "schematics": {},
        "architect": {
          "build": {
            "builder": "@angular-devkit/build-angular:browser",
            "options": {
              "outputPath": "dist/insight-ui",
              "index": "src/index.html",
              "main": "src/main.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "src/tsconfig.app.json",
              "assets": [
                "src/favicon.ico",
                "src/assets"
              ],
              "styles": [
                {
                  "input": "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
                },
                {
                  "input": "node_modules/font-awesome/scss/font-awesome.scss"
                },
                "src/styles.scss",
                {
                  "input": "./node_modules/bootstrap/dist/css/bootstrap.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-grid.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-balham.css"
                },
                {
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-balham-dark.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-material.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-fresh.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-dark.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-blue.css"
                },{
                  "input": "./node_modules/ag-grid/dist/styles/ag-theme-bootstrap.css"
                }
                
              ],
              "scripts": []
            },
            "configurations": {
              "production": {
                "fileReplacements": [
                  {
                    "replace": "src/environments/environment.ts",
                    "with": "src/environments/environment.prod.ts"
                  }
                ],
                "optimization": true,
                "outputHashing": "all",
                "sourceMap": false,
                "extractCss": true,
                "namedChunks": false,
                "aot": true,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true
              }
            }
          },
          "serve": {
            "builder": "@angular-devkit/build-angular:dev-server",
            "options": {
              "browserTarget": "insight-ui:build"
            },
            "configurations": {
              "production": {
                "browserTarget": "insight-ui:build:production"
              }
            }
          },
          "extract-i18n": {
            "builder": "@angular-devkit/build-angular:extract-i18n",
            "options": {
              "browserTarget": "insight-ui:build"
            }
          },
          "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
              "main": "src/test.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "src/tsconfig.spec.json",
              "karmaConfig": "src/karma.conf.js",
              "styles": [
                {
                  "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
                },
                "src/styles.css"
              ],
              "scripts": [],
              "assets": [
                "src/favicon.ico",
                "src/assets"
              ]
            }
          },
          "lint": {
            "builder": "@angular-devkit/build-angular:tslint",
            "options": {
              "tsConfig": [
                "src/tsconfig.app.json",
                "src/tsconfig.spec.json"
              ],
              "exclude": [
                "**/node_modules/**"
              ]
            }
          }
        }
      },
      "insight-ui-e2e": {
        "root": "e2e/",
        "projectType": "application",
        "architect": {
          "e2e": {
            "builder": "@angular-devkit/build-angular:protractor",
            "options": {
              "protractorConfig": "e2e/protractor.conf.js",
              "devServerTarget": "insight-ui:serve"
            },
            "configurations": {
              "production": {
                "devServerTarget": "insight-ui:serve:production"
              }
            }
          },
          "lint": {
            "builder": "@angular-devkit/build-angular:tslint",
            "options": {
              "tsConfig": "e2e/tsconfig.e2e.json",
              "exclude": [
                "**/node_modules/**"
              ]
            }
          }
        }
      }
    },
    "defaultProject": "insight-ui",
    "schematics": {
      "@schematics/angular:component": {
        "styleext": "scss"
      },
      "@schematics/angular": {
        "styleext": "scss"
      }
    }
  }